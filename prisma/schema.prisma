generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Profile{
  id Int @id @default(autoincrement())
  name String 
  email String @unique
  password String
  userId String @unique @default(uuid())
  createdAt DateTime @default(now())

  @@map("profile")
}