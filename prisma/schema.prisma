generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Profile{
  id String @id @default(uuid())
  name String 
  email String @unique
  password String
  createdAt DateTime @default(now())

  @@map("profile")
}

model Courses{
  id String @id @default(uuid())
  images String
  name String
  description String

  @@map("courses")
}